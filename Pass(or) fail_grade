#include<stdio.h>
void main()
{
int m1,m2,m3,m4,m5,tot;
float avg;
char res[5];
clrscr();
//inputs
printf("Enter the values for m1,m2,m3,m4,m5");
scanf("%d%d%d%d%d",&m1,&m2,&m3,&m4,&m5);
tot=m1+m2+m3+m4+m5;
if(m1>=35&&m2>=35&&m3>=35&&m4>=35&&m5>=35)
{
res[0]='p';
res[1]='a';
res[2]='s';
res[3]='s';
res[4]='\0';
}
else
{
res[0]='f';
res[1]='a';
res[2]='i';
res[3]='l';
res[4]='\0';
}
printf("\n total: %d", tot);
printf("\nresult:%s", res);
getch();
}
